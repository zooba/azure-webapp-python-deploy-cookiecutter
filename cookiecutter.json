{
    "python_version": ["3.5.2", "2.7.12"],
    "cpu_arch": ["x64", "x86"],
    "site_name": "webapp",
    "static_assets_directory": "static",
    "post_deployment_scripts_directory": "post-deployment",
    "requirements_filename": "requirements.txt",
    "main_module": "app",
    "script_dir": ".azure",
    "site_type": ["socket", "WSGI"],
    "socket_python_arguments":
        "{% if cookiecutter.site_type == 'socket' %}-m {{ cookiecutter.main_module }} --port %HTTP_PLATFORM_PORT%{% else %}N/A{% endif %}",
    "wsgi_app_object":
        "{% if cookiecutter.site_type == 'WSGI' %}{{ cookiecutter.main_module }}.wsgi_app{% else %}N/A{% endif %}",
    "azure_resource_group": "{{ cookiecutter.site_name }}",
    "azure_region": ["East US", "East US 2", "Central US", "North Central US", "South Central US", "West Central US", "West US", "West US 2",
                     "Canada East", "Canada Central", "Brazil South", "North Europe", "West Europe", "Germany Central", "Germany Northeast",
                     "UK West", "UK South", "Southeast Asia", "East Asia", "Australia East", "Australia Southeast", "Central India",
                     "West India", "South India", "Japan East", "Japan West", "China East", "China North"]
}